{
	"compilerOptions": {
		"outDir": "dist",
		"lib": ["es6"],
		"target": "es6",
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"declaration": true,
		"sourceMap": true,
		"strict": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"noUncheckedIndexedAccess": true,
		"allowSyntheticDefaultImports": true
	},
	"files": ["src/memoize.ts"]
}
