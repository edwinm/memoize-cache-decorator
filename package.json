{
	"name": "memoize-cache-decorator",
	"version": "1.3.3",
	"description": "Cache the result of a method or getter for better performance. Supports timeout and clearing of the cache.",
	"license": "MIT",
	"main": "dist/memoize.js",
	"types": "dist/memoize.d.ts",
	"sideEffects": false,
	"scripts": {
		"prepare": "husky install",
		"build": "tsc",
		"test": "tsc -p test/tsconfig.json -outDir test && npx jest test/test.js",
		"coveralls": "jest --coverage test/test.js && cat ./coverage/lcov.info | coveralls",
		"prettier": "prettier --config .prettierrc.json src/*.ts *.json *.cjs --write",
		"lint": "eslint . --ext .ts"
	},
	"keywords": [
		"memoize",
		"decorator",
		"cache",
		"clear",
		"reset",
		"timeout",
		"ttl",
		"expire",
		"typescript"
	],
	"author": {
		"name": "Edwin Martin",
		"email": "edwin@bitstorm.org",
		"url": "https://bitstorm.org/"
	},
	"files": [
		"src/memoize.ts",
		"dist/memoize.js",
		"dist/memoize.d.ts",
		"dist/memoize.js.map"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/edwinm/memoize-cache-decorator.git"
	},
	"devDependencies": {
		"@types/jest": "^29.5.2",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"coveralls": "^3.1.1",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"prettier": "^2.8.8",
		"pretty-quick": "^3.1.3",
		"typescript": "^5.1.6"
	}
}
